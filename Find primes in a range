import math

def is_prime(n):
    if n <= 1:
        return False
    for i in range(2, int(math.sqrt(n)) + 1):
        if n % i == 0:
            return False
    return True

def print_primes_in_range(a, b):
    for num in range(a, b + 1):
        if is_prime(num):
            print(num, end=' ')
